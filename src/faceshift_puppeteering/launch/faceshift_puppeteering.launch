<launch>
<arg name="IP" default="localhost"/>
<arg name="Port" default="33433"/>
<arg name="fp_mode" default="0"/>
<param name="IP" value="$(arg IP)"/>
<param name="Port" type="str" value="$(arg Port)"/>
<param name="fb_mode" value="$(arg fp_mode)"/>
<node pkg="faceshift_puppeteering" name="faceshift_puppeteering" type="faceshift_puppeteering_node"/>
<node pkg="faceshift_puppeteering" name="faceshift_mapper" type="blendshapes_mapper.py" output="screen"/>
</launch>
